{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next .swc out",
    "lint": "next lint",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty -p tsconfig.typecheck.json",
    "typecheck": "npm run check-types",
    "test": "npm run test:unit",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "db:migrate": "node db/scripts/apply-sql.js migrations",
    "db:seed": "node db/scripts/apply-sql.js seeds",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "data-retention:apply": "node scripts/apply-data-retention.js",
    "test:jest": "jest --runInBand",
    "test:playwright": "playwright test",
    "retention:run": "node scripts/run-data-retention.mjs"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "classnames": "^2.5.1",
    "next": "^14.1.0",
    "next-seo": "^6.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-jsx-plugin-postcss": "^4.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@next/swc-wasm-nodejs": "^14.1.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.10",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "@next/swc-win32-x64-msvc": "^14.1.0"
  },
  "license": "ISC"
}
